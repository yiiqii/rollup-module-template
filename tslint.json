{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "avoid-template"
    ],
    "no-bitwise": false,
    "no-var-requires": false,
    "unified-signatures": false,
    "max-line-length": [true, 720],
    "no-reference": false,
    "no-console": [true, "log"],
    "no-empty": false,
    "no-namespace": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false
  },
  "rulesDirectory": []
}
